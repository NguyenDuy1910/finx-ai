        "expiration_date",
        "end_date"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "day_until_expire",
      "type": "int",
      "description": "Number of days remaining until expiry.",
      "terms": [
        "days_left",
        "days_remaining"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "expire_status",
      "type": "string",
