      "terms": [
        "expiration_status",
        "validity_status"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "event_type",
      "type": "string",
      "description": "Type of event or notification related to biometrics.",
      "terms": [
        "notification_type",
        "alert_type"
      ],
      "primary_key": false,
      "foreign_key": false
