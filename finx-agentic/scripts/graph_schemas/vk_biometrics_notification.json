{
  "name": "vk_biometrics_notification",
  "database": "non_prod_uat_gold_zone",
  "description": "This table stores notifications related to biometric data and their expiry for clients.",
  "columns": [
    {
      "name": "cif",
      "type": "string",
      "description": "Unique identifier for the client.",
      "terms": [
        "customer_id",
        "client_identifier"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "client_status",
      "type": "string",
      "description": "Current status of the client.",
      "terms": [
        "status",
        "account_status"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "national_id",
      "type": "string",
      "description": "National identification number of the client.",
      "terms": [
        "id_number",
        "identity_number"
      ],
      "primary_key": false,
      "foreign_key": true
    },
    {
      "name": "nid_type",
      "type": "string",
      "description": "Type of national identification document.",
      "terms": [
        "id_type",
        "identity_type"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "expiry_date",
      "type": "date",
      "description": "Date when the biometric data expires.",
