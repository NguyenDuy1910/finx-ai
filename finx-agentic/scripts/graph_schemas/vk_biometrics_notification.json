      "terms": [
        "execution_date",
        "process_date"
      ],
      "primary_key": false,
      "foreign_key": false
    },
    {
      "name": "processed_at",
      "type": "timestamp",
      "description": "Timestamp when the record was processed.",
      "terms": [
        "timestamp",
        "processed_time"
      ],
      "primary_key": false,
      "foreign_key": false
    }
  ],
  "entity": {
    "name": "BiometricNotification",
    "domain": "customer_management",
    "synonyms": [
      "BiometricAlert",
      "NotificationRecord"
    ]
